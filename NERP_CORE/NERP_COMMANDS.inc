			fwrite( ban, var );
			new var1[ 128 ];
			format( var1, 128,"Razlog=5/5 Warnova\n" );
			fwrite( ban, var1 );
			fclose( ban );
			ServerInfo[ BanovanihKorisnika ]++;
			Kickovanje( id, ""col_white"Ti si banovan sa servera. Unban zatrazi na DISCORDU: https://discord.gg/XkcSmSxFMe");
