	if( PlayerInfo[ playerid ][ xAdmin ] >= 6  )
	{
		if( !AdminDuty[ playerid ] ) return GRESKA( playerid, "Niste u mogucnosti koristiti ovo - morate biti administrator na duznosti!" );
