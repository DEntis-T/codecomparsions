	if( id == playerid ) return greska(playerid,"Ne mozes sam sebi pomoci - nepokretan si!");
	if( !IsPlayerNearPlayer( 5.0, playerid, id ) ) return GRESKA( playerid, "Niste blizu igraca kojem zelite pomoci." );
