		AdminVozilo[ playerid ] = Dimi_CreateVehicle( -1, -1, -1, -1, -1, idv, Poz[ 0 ], Poz[ 1 ], Poz[ 2 ], 0.0, b1, b2, -1 );
		LinkVehicleToInterior( AdminVozilo[ playerid ], GetPlayerInterior( playerid ) );
