CMD:uniforma( playerid, params[] )
{
	if( IsPlayerInAnyVehicle( playerid ) ) return GRESKA( playerid, "Ne mozes da uzmes opremu dok si u vozilu." );
	new id = JobUniformPlace( playerid );
