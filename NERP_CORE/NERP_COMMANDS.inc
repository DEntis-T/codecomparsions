				Dimi_SetPlayerPos( ID, vehPos[ 0 ], vehPos[ 1 ], vehPos[ 2 ] );
			}
			else
				GRESKA( playerid, "Taj igrac nije u vasem autu !");
		}
	}
	else
		GRESKA( playerid, "Nepravilan ID/DioImena!");
	return 1;
}
