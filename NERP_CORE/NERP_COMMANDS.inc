	}
	else if( AdminVozilo[ playerid ] != -1 )
	{
		SendInfoMessage( playerid, "Vozilo ID: %d je unisteno.", AdminVozilo[ playerid ] );
		Dimi_DestroyVehicle( AdminVozilo[ playerid ] );
		AdminVozilo[ playerid ] = -1;
	}
	return true;
