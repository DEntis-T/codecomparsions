CMD:nitro( playerid, params[] )
{
	if( PlayerInfo[ playerid ][ xAdmin ] >= 3 || PlayerInfo[ playerid ][ xVIPLevel ] >= 3 || PlayerInfo[ playerid ][ xPromoter ] >= 1)
	{
		if( PlayerInfo[ playerid ][ xAdmin ] >= 3)
