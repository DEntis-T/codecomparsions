						format( string, sizeof( string ), "* %s salje poruku.", RPImeIgraca( playerid ) );
						PorukaRadius( 20.0, playerid, string, LJUBICASTA );
						return 1;
					}
				}
			}
		}
	}
	SendClientMessagee( playerid, 0xAFAFAFAA, "  Slanje poruke neuspelo...");
	return 1;
}
