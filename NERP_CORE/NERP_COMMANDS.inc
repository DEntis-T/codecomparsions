			Dimi_DisablePlayerCheckpoint( playerid );
			CheckpointStatus[ playerid ] = 0;
			SetVehicleToRespawn( vehicleid );
			SendJobMessage( playerid, "Uspjesno ste prekinuli sa poslom, sada mozete ostaviti uniformu ili opet raditi." );
