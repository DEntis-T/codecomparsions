			MoveDynamicObject(rudnikgate1,490.4233, 774.5048, 1003.1907,3.0);//gornja spustena
			MoveDynamicObject(rudnikgate2,490.5960, 775.6660, 969.7675,0.2);//donja ograda se lagano dize
