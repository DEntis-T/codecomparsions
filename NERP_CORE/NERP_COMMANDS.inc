						announcement( playerid, string, 5000, 1);
						PlayerInfo[ playerid ][ xTelefonBon ] -= smscost;
						BussinesTelekomMoney( smscost );
						PlayerPlaySound( playerid, 1052, 0.0, 0.0, 0.0 );
