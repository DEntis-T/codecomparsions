			{
				SendInfoMessage(playerid, "Droga je spremna za branje!" );
			}
			if( DrogaStab[ plantid ][ wHoursPassed ] > 8 && DrogaStab[ plantid ][ wHoursPassed ] <= 9 )
			{
				SendInfoMessage(playerid, "Ova sadnica je skoro prezrela i od nje bi dobili 1 gram!" );
