	if( PlayerInfo[ playerid ][ xMute ] != 0 ) return GRESKA( playerid, "Ne mozete pricati, usutkani ste.");
	new phonenumb, result[128], string[234], igrac;
	if( sscanf(params, "is[128]", phonenumb, result ) )
